<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.admon.entity.reportes.OrganizacionAdmon" table="ADMON_ORGANIZACION">
    <id name="organizacionAdmonId" type="java.lang.Integer">
      <column name="ORGANIZACION_ID"/>
      <generator class="sequence">
        <param name="sequence">ORGANIZACION_SEQ</param>
      </generator>
    </id>
    <property name="nombre" type="string">
      <column name="NOMBRE"/>
    </property>
    <property name="eliminadoId" type="java.lang.Integer">
      <column name="ELIMINADO_ID"/>
    </property>
    <property name="habilitado" type="java.lang.Integer">
      <column name="HABILITADO"/>
    </property>
  </class>
</hibernate-mapping>
