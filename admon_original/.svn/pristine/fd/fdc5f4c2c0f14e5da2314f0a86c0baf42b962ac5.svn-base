<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.admon.pkg.entity.EstadoCuentaDetalleRS" >
        <id name="identificador" type="java.lang.Integer" column="IDENTIFICADOR"/>
        <property name="organizacionId" type="java.lang.Integer" column="ORGANIZACION_ID"/>
        <property name="nombreEvento" type="java.lang.String" column="NOMBRE_EVENTO"/>
        <property name="nombrePresentacion" type="java.lang.String" column="PRESENTACION_NOMBRE"/>
        <property name="canalVenta" type="java.lang.String" column="CLAVE_CANAL_VENTA"/>
        <property name="mesFactura" type="java.lang.String" column="MES_FACTURA"/>
        <property name="noBoletos" type="java.lang.Integer" column="CON_BOLETOS"/>
        <property name="balance" type="java.lang.Double" column="BALANCE"/>
        <property name="total" type="java.lang.Double" column="TOTAL"/>
        <property name="monto" type="java.lang.Double" column="MONTO"/>
        <property name="descuento" type="java.lang.Double" column="DESCUENTO"/>
        <property name="cargoXServicio" type="java.lang.Double" column="CARGO_SERVICIO"/>
        <property name="comisionTaasgo" type="java.lang.Double" column="GANANCIAS"/>
    </class>
</hibernate-mapping>