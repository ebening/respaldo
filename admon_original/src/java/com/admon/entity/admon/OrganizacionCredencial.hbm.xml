<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.admon.entity.admon.OrganizacionCredencial" table="ORGANIZACION_CREDENCIAL">
        <id name="organizacionCredencialId" type="java.lang.Integer">
            <column name="ORGANIZACION_CREDENCIAL_ID"/>
            <generator class="increment"/>
        </id>
        <property name="organizacionId" type="java.lang.Integer">
            <column name="ORGANIZACION_ID"/>
        </property>
        <property name="nombre" type="string">
            <column name="NOMBRE"/>
        </property>
        <property name="domain" type="string">
            <column name="DOMAIN"/>
        </property>
        <property name="aplicacionId" type="java.lang.Integer">
            <column name="APLICACION_ID"/>
        </property>
        <property name="idapp" type="string">
            <column name="IDAPP"/>
        </property>
        <property name="idrecapcha" type="string">
            <column name="IDRECAPCHA"/>
        </property>
        <property name="idanalytics" type="string">
            <column name="IDANALYTICS"/>
        </property>
        <property name="estatusId" type="java.lang.Integer">
            <column name="ESTATUS_ID"/>
        </property>
        <property name="eliminadoId" type="java.lang.Integer">
            <column name="ELIMINADO_ID"/>
        </property>
        <property name="creacionFecha" type="timestamp">
            <column name="CREACION_FECHA"/>
        </property>
        <property name="creacionUsuario" type="java.lang.Integer">
            <column name="CREACION_USUARIO"/>
        </property>
        <property name="modificacionFecha" type="timestamp">
            <column name="MODIFICACION_FECHA"/>
        </property>
        <property name="modificacionUsuario" type="java.lang.Integer">
            <column name="MODIFICACION_USUARIO"/>
        </property>
    </class>
</hibernate-mapping>

