<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.admon.entity.admon.Pagina" table="PAGINA">
        <id name="paginaId" type="java.lang.Integer">
            <column name="PAGINA_ID"/>
            <generator class="sequence">
                <param name="sequence">PAGINA_SEQ</param> 
            </generator>
        </id>
        <property name="nombre" type="string">
            <column name="NOMBRE"/>
        </property>
        <property name="anidar" type="java.lang.Integer">
            <column name="ANIDAR"/>
        </property>
        <property name="url" type="string">
            <column name="URL"/>
        </property>
        <property name="orden" type="java.lang.Integer">
            <column name="ORDEN"/>
        </property>
        <property name="noIncluirEnMenu" type="java.lang.Integer">
            <column name="NO_INCLUIR_EN_MENU"/>
        </property>
        <property name="privado" type="java.lang.Integer">
            <column name="PRIVADO"/>
        </property>
        <property name="sinGrid" type="java.lang.Integer">
            <column name="SIN_GRID"/>
        </property>
        <property name="individual" type="java.lang.Integer">
            <column name="INDIVIDUAL"/>
        </property>
        <property name="estatusId" type="java.lang.Integer">
            <column name="ESTATUS_ID"/>
        </property>
        <property name="eliminadoId" type="java.lang.Integer">
            <column name="ELIMINADO_ID"/>
        </property>
        <property name="creacionFecha" type="timestamp">
            <column name="CREACION_FECHA"/>
        </property>
        <property name="creacionUsuario" type="java.lang.Integer">
            <column name="CREACION_USUARIO"/>
        </property>
        <property name="modificacionFecha" type="timestamp">
            <column name="MODIFICACION_FECHA"/>
        </property>
        <property name="modificacionUsuario" type="java.lang.Integer">
            <column name="MODIFICACION_USUARIO"/>
        </property>
        <property name="icono" type="string">
            <column name="ICONO"/>
        </property>
    </class>
</hibernate-mapping>

