<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.admon.pkg.entity.EstadoCuentaRS">
        <id name="organizacionId" type="java.lang.Integer" column="ORGANIZACION_ID"/>
        <property name="eventoId" type="java.lang.Integer" column="EVENTO_ID"/>
        <property name="fechaPagoInicio" type="java.sql.Date" column="FECHA_INICIO"/>
        <property name="fechaPagoFin" type="java.sql.Date" column="FECHA_FIN"/>
        <property name="nombreOrganizacion" type="java.lang.String" column="NOMBRE"/>
        <property name="nombreComercial" type="java.lang.String" column="NOMBRE_COMERCIAL"/>
        <property name="RFC" type="java.lang.String" column="RFC"/>
        <property name="domicilio" type="java.lang.String" column="DIRECCION"/>
        <property name="nombreEvento" type="java.lang.String" column="NOMBRE_EVENTO"/>
        <property name="noBoletos" type="java.lang.Integer" column="NO_BOLETOS"/>
        <property name="noCortesias" type="java.lang.Integer" column="BOLETOS_CORTESIA"/>
        <property name="noCancelaciones" type="java.lang.Integer" column="BOLETOS_CANCELACION"/>
        <property name="noBoletoDuro" type="java.lang.Integer" column="BOLETOS_DURO"/>
        <property name="total" type="java.lang.Double" column="TOTAL"/>
        <property name="monto" type="java.lang.Double" column="MONTO"/>
        <property name="descuento" type="java.lang.Double" column="DESCUENTO"/>
        <property name="cargoXServicio" type="java.lang.Double" column="CARGO_SERVICIO"/>
        <property name="comisionTaasgo" type="java.lang.Double" column="GANANCIAS"/>
    </class>
</hibernate-mapping>
