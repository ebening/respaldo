<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.admon.entity.admon.ConfiguracionFormaPago" table="CONFIG_FORMA_PAGO">
    <id name="configuracionFormaPagoId" type="java.lang.Integer"> 
        <column name="CONFIG_FORMA_PAGO_ID"/>
        <generator class="sequence">
            <param name="sequence">CONFIG_FORMA_PAGO_ID_SEQ</param> 
        </generator>
     </id>
    <property name="urlSite" type="string">
      <column name="URL_SITE"/>
    </property>
    <property name="urlReturn" type="string">
      <column name="URL_RETURN"/>
    </property>
    <property name="urlCancel" type="string">
      <column name="URL_CANCEL"/>
    </property>
    <property name="username" type="string">
      <column name="USERNAME"/>
    </property>
    <property name="password" type="string">
      <column name="PASSWORD"/>
    </property>
    <property name="signature" type="string">
      <column name="SIGNATURE"/>
    </property>
    <property name="currency" type="string">
      <column name="CURRENCY"/>
    </property>
    <property name="contractId" type="java.lang.Integer">
      <column name="CONTRACT_ID"/>
    </property>
    <property name="conectionMode" type="string">
      <column name="CONNECTION_MODE"/>
    </property>
    <property name="claveFormaPago" type="string">
      <column name="CLAVE_FORMA_PAGO"/>
    </property>
    <property name="secret" type="string">
      <column name="SECRET"/>
    </property>
    <property name="clientId" type="string">
      <column name="CLIENT_ID"/>
    </property>
     <property name="urlOauth" type="string">
      <column name="URL_OAUTH"/>
    </property>
    <property name="urlStc" type="string">
      <column name="URL_STC"/>
    </property>
    <property name="merchantId" type="string">
      <column name="MERCHANT_ID"/>
    </property>
    <property name="afiliacionDefault" type="java.lang.Integer">
        <column name="AFILIACION_DEFAULT"/>
    </property>
    <property name="estatusId" type="java.lang.Integer">
        <column name="ESTATUS_ID"/>
    </property>
    <property name="eliminadoId" type="java.lang.Integer">
        <column name="ELIMINADO_ID"/>
    </property>
    <property name="creacionFecha" type="timestamp">
        <column name="CREACION_FECHA"/>
    </property>
    <property name="creacionUsuario" type="java.lang.Integer">
        <column name="CREACION_USUARIO"/>
    </property>
    <property name="modificacionFecha" type="timestamp">
        <column name="MODIFICACION_FECHA"/>
    </property>
    <property name="modificacionUsuario" type="java.lang.Integer">
        <column name="MODIFICACION_USUARIO"/>
    </property>
  </class>
</hibernate-mapping>
