<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.admon.entity.admon.SeguridadPerfilRol" table="SEGURIDAD_PERFIL_ROL">
        <id name="seguridadPerfilRolId" type="java.lang.Integer">
            <column name="PERFIL_ROL_ID"/>
            <generator class="sequence">
                <param name="sequence">PERFIL_ROL_SEQ</param> 
            </generator>
        </id>
        <property name="perfilId" type="java.lang.Integer">
            <column name="PERFIL_ID"/>
        </property>
        <property name="rolId" type="java.lang.Integer">
            <column name="ROL_ID"/>
        </property>
        <property name="aplicacionId" type="java.lang.Integer">
            <column name="APLICACION_ID"/>
        </property>
        <property name="creacionFecha" type="timestamp">
            <column name="CREACION_FECHA"/>
        </property>
        <property name="creacionUsuario" type="java.lang.Integer">
            <column name="CREACION_USUARIO"/>
        </property>
        <property name="modificacionFecha" type="timestamp">
            <column name="MODIFICACION_FECHA"/>
        </property>
        <property name="modificacionUsuario" type="java.lang.Integer">
            <column name="MODIFICACION_USUARIO"/>
        </property>
    </class>
</hibernate-mapping>

