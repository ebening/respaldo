<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.admon.entity.admon.ParcializacionSantander" table="PARCIALIZACION_SANTANDER">
        <id name="parcializacionSantanderId" type="java.lang.Long">
            <column name="PARCIALIZACION_SANTANDER_ID"/>
            <generator class="sequence">
                <param name="sequence">PARCIALIZACION_SANTANDER_SEQ</param> 
            </generator>
        </id>
        <property name="afiliacion" type="string">
            <column name="AFILIACION"/>
        </property>
        <property name="parcializacion" type="java.lang.Long">
            <column name="PARCIALIZACION"/>
        </property>
        <property name="merchant" type="string">
            <column name="MERCHANT"/>
        </property>
        <property name="semilla3ds" type="string">
            <column name="SEMILLA_3DS"/>
        </property>
        <property name="claveInstitucionBancaria" type="string">
            <column name="CLAVE_INSTITUCION_BANCARIA"/>
        </property>
        <property name="creacionFecha" type="timestamp">
            <column name="CREACION_FECHA"/>
        </property>
        <property name="creacionUsuario" type="java.lang.Integer">
            <column name="CREACION_USUARIO"/>
        </property>
        <property name="modificacionFecha" type="timestamp">
            <column name="MODIFICACION_FECHA"/>
        </property>
        <property name="modificacionUsuario" type="java.lang.Integer">
            <column name="MODIFICACION_USUARIO"/>
        </property>
    </class>
</hibernate-mapping>

