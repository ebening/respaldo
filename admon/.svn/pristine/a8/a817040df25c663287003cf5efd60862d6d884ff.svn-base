<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.admon.entity.admon.PayPal" table="EVENTO_FORMA_PAGO">
    <id name="payPalId" type="java.lang.Integer"> 
        <column name="CONFIG_FORMA_PAGO_ID"/>
    </id>
    <property name="eventoId" type="java.lang.Integer">
      <column name="EVENTO_ID"/>
    </property>
    <property name="creacionFecha" type="timestamp">
        <column name="CREACION_FECHA"/>
    </property>
    <property name="creacionUsuario" type="java.lang.Integer">
        <column name="CREACION_USUARIO"/>
    </property>
  </class>
</hibernate-mapping>
